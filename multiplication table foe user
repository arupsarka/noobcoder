import java.util.Scanner;

public class Main {
    static void logic(){
        System.out.println("Enter your number");
        Scanner sc =new Scanner(System.in);
        int z=sc.nextInt();
        for (int i=1;i<=10;i++) {
            System.out.printf("%d X %d = %d \n",z ,i,z*i);
        }
    }
    public static void main(String[] args) {
        logic();
    }
}
